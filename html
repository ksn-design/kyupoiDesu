  <script type="text/javascript">
    var readyBids = {
      prebid: false,
      google: false,
      amazon: false
    };
    var　failSafeTimeout = 3e3;
    var launchAdServer = function () {
      if (!readyBids.amazon || !readyBids.prebid) {
        return;
      }
      requestAdServer();
    };
    var requestAdServer = function () {
      if (!readyBids.google) {
        readyBids.google = true;
        googletag.cmd.push(function () {
